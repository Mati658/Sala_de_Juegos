{
  "hosting": {
    "public": "./dist/sala-de-juegos/browser",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/head_soccer/**", 
        "destination": "/head_soccer/Index.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
